<%= form_for @cat do |f| %>
  <div>
  <%= f.label :name %>
  <%= f.text_field :name %>
  </div>
  <%= f.label :sex %>
  <%= f.text_field :sex %>
  </div>  
  <div>
  <%= f.label :color %>
  <%= f.text_area :color %>
  </div>
  <div>
  <%= f.label :age %>
  <%= f.number_field :year_released, min: 1886, max: Date.today.year %>
  </div>
  <div>
  <%= f.label :rating %>
  <%= f.number_field :rating, min: 1, max: 5 %>
  </div>
  <%= f.submit %>
<% end %>